{
	"compilerOptions": {
		"allowJs": false,
		"declaration": true,
		"lib": [
			"DOM",
			"DOM.Iterable",
			"ES2020"
		],
		"module": "ES2020",
		"target": "ES2020",
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"removeComments": true,
		"skipLibCheck": false,
		"strict": true,
		"strictNullChecks": true,
		"alwaysStrict": true,
	},
	"include": [
		"."
	]
}
